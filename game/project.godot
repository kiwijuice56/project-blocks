config_version=5

[application]

config/name="project-blocks"
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

Ref="*res://main/autoload/ref.gd"
ItemMap="*res://main/autoload/item_map.gd"

[display]

window/size/viewport_width=300
window/size/viewport_height=256
window/size/window_width_override=600
window/size/window_height_override=512
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/vsync/vsync_mode=0

